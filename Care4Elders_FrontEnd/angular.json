{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "Care4Elders_FrontEnd": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/care4-elders-front-end",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/rose-red.css",
              "src/styles.css",
              "src/assets/lib/animate/animate.min.css",
              "src/assets/lib/owlcarousel/assets/owl.carousel.min.css",
              "src/assets/lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/style.css"
            ],
            "scripts": [
              "src/assets/lib/jquery/jquery.min.js",
              "src/assets/lib/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/lib/owlcarousel/owl.carousel.min.js",
              "src/assets/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js",
              "src/assets/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "fonts": {
                  "inline": false
                }
              }
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "Care4Elders_FrontEnd:build:production"
            },
            "development": {
              "buildTarget": "Care4Elders_FrontEnd:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              }
            ],
            "styles": [
              "@angular/material/prebuilt-themes/rose-red.css",
              "src/styles.css",
              "src/assets/lib/animate/animate.min.css",
              "src/assets/lib/owlcarousel/assets/owl.carousel.min.css",
              "src/assets/lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/style.css"
            ],
            "scripts": [
              "src/assets/lib/jquery/jquery.min.js",
              "src/assets/lib/bootstrap/js/bootstrap.bundle.min.js",
              "src/assets/lib/owlcarousel/owl.carousel.min.js",
              "src/assets/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js",
              "src/assets/js/main.js"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "7d1b2714-0415-42d4-9188-5ad458866a1b",
    "schematicCollections": [
      "angular-eslint"
    ]
  }
}
